{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest/globals": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "plugins": ["jest"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "jest/no-disabled-tests": "warn",
        "jest/no-identical-title": "error",
        "jest/valid-expect": "error"
    }
}
